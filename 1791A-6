#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    string str="codeforces";
    while(t--){
        char ch;
        cin>>ch;
        bool chk=false;
        for(int i=0;i<str.size();i++){
            if(ch==str[i]){
                chk=true;
                break;
            }
            else{
                chk=false;
            }
        }
        if(chk){
            cout<<"yes"<<endl;
        }
        else{
            cout<<"no"<<endl;
        }
    }
return 0;
}
