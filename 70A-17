#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
            int a,b,cntm=0,cntc=0;
    for(int i=0;i<n;i++){

        cin>>a>>b;
        if(a>b){
            cntm++;
        }
        else if(a<b){
            cntc++;
        }
    }
    if(cntm>cntc){
        cout<<"Mishka";
    }
    else if(cntc>cntm){
        cout<<"Chris";
    }
    else{
        cout<<"Friendship is magic!^^";
    }
return 0;
}
