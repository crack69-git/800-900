#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    char ch[n];
    vector<char>v;
    for(int i=0;i<n;i++){
        cin>>ch[i];
        v.push_back(ch[i]);
    }
    int a=count(v.begin(),v.end(),'A');
    int d=count(v.begin(),v.end(),'D');
    if(a>d){
        cout<<"Anton";
    }
    else if(a<d){
        cout<<"Danik";
    }
    else{
        cout<<"Friendship";
    }
return 0;
}
