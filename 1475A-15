#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;
    cin>>t;
    while(t--){
    	long long n;
        cin>>n;
        bool found=false;
        if(n%2!=0){
        	found=true;
        }
        else{
       	 while(n>=1){
            	n/=2;
            	if(n%2!=0 && n!=1){
                	found=true;
                    break;
                }
            }
            
        }
        if(found){
        	cout<<"yes"<<endl;
        }
        else{
        	cout<<"no"<<endl;
            }
    }
return 0;
}
