#include<bits/stdc++.h>
using namespace std;
int main(){
    long long n;\
    int c=0;
    cin>>n;
    while (n > 0) {
        int digit = n % 10;
        if (digit == 4 || digit == 7) {
            c++;
        }
        n /= 10;
    }

    if (c == 4 || c == 7) {
        cout << "YES";
    } else {
        cout << "NO";
    }
return 0;
}
