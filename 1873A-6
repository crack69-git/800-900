#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    cin.ignore();
    while(t>0){
        string str;
        getline(cin,str);
        if(str=="abc"){
            cout<<"Yes"<<endl;
        }
        else if(str=="acb"){
            cout<<"yes"<<endl;
        }
        else if(str=="bac"){
            cout<<"yes"<<endl;
        }
        else if(str=="cba"){
            cout<<"yes"<<endl;
        }
        else{
            cout<<"no"<<endl;
        }
        t--;
    }
return 0;
}
