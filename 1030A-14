#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    bool cnt=false;
    cin>>n;
    int arr[n];
    vector<int>v;
    for(int i=0;i<n;i++){
        cin>>arr[i];
        v.push_back(arr[i]);
        if(v[i]==0){
            cnt=true;
        }
        else if(v[i]==1){
            cnt=false;
            break;
        }
    }
    if(cnt){
        cout<<"easy";
    }
    else{
        cout<<"hard";
    }
return 0;
}
