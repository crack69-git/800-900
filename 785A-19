#include<bits/stdc++.h>
using namespace std;
int main(){
    int t,cnt=0;
    cin>>t;
    while(t>=0){
        string str;
        getline(cin,str);
        if(str=="Tetrahedron"){
            cnt+=4;
        }
        else if(str=="Cube"){
            cnt+=6;
        }
        else if(str=="Octahedron"){
            cnt+=8;
        }
        else if(str=="Dodecahedron"){
            cnt+=12;
        }
        else if(str=="Icosahedron"){
            cnt+=20;
        }
        t--;
    }
    cout<<cnt;
return 0;
}
