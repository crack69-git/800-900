#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,cnt=0;
    cin.ignore();
    string str;
    getline(cin,str);
    set<char>s;
    for(int i=0;i<str.size();i++){
        n=str[i]-'\0';
        if(n>=97 && n<=122){
            s.insert(str[i]);
        }
    }
    cout<<s.size();
return 0;
}
