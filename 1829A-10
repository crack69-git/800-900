#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    cin.ignore();
    while(t--){
        string str;
        getline(cin,str);
        int cnt=0;
        string st="codeforces";
        for(int i=0;i<str.size();i++){
            if(str[i]!=st[i]){
                cnt++;
            }
        }
        cout<<cnt<<endl;
    }
return 0;
}
