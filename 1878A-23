#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n,k,x,cnt=0;
        cin>>n>>k;
        int arr[n];
        bool found=false;
        for(int i=0;i<n;i++){
            cin>>arr[i];
            if(k==arr[i]){
                found=true;
            }
        }
        if(found){
            cout<<"yes"<<endl;
        }
        else{
            cout<<"no"<<endl;
        }

    }
return 0;
}
