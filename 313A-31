#include <iostream>
#include <cmath>
#include <string>

using namespace std;

int main() {
    string n;
    cin >> n;

    if (n[0] != '-') {
        cout << n << endl;
    } else {
        int num = stoi(n);
        int last_removed = stoi(n.substr(0, n.size() - 1));
        int second_last_removed = stoi(n.substr(0, n.size() - 2) + n[n.size() - 1]);
        cout << max(last_removed, second_last_removed) << endl;
    }

    return 0;
}
