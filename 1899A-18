//game with integers
#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;
    cin>>t;
    while(t--){
    	int n;
        cin>>n;
        bool ck=false;
        for(int i=0;i<=n;++i){
        	if((n+1)%3==0 ||((n-i)>=0 && (n-1)%3==0)){
            	ck=true;
                break;
            }
        }
        if(ck){
        	cout<<"First"<<endl;
        }
        else{
        	cout<<"Second"<<endl;
        }
    }
    return 0;
}
