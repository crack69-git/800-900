#include<bits/stdc++.h>
using namespace std;
int main(){
    int a,b,re,cnt=0;
    cin>>a>>b;
    if(a>b){
        cnt=b;
        re=(a-b);
        if(re%2==0){
            re/=2;
        }
        else{
            re--;
            re/=2;
        }
    }
    else{
        cnt=a;
        re=(b-a);
        if(re%2==0){
           re/=2;
        }
        else{
            re--;
            re/=2;
        }
    }
    cout<<cnt<<" "<<re;;
return 0;
}
